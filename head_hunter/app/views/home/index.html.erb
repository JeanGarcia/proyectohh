  <!--Registrarse como Aspirante-->

  <div class="modal fade" id="myModalreg" role="dialog" >
    <div class="modal-dialog ">
    
      <!-- Modal content-->
      <div class="modal-content">

        <div class="modal-header" style="color: white; background-color: #006bb3">
          <button type="button" class="close" data-dismiss="modal">&times;</button>

          <h4 class="modal-title text-center" >Registro - Aspirante</h4>
        </div>

    <div id="mod_reg" class="modal-body" >
<!-- Body Content-->
     <form class="form-horizontal">

      <div class="form-group">
          <label>Nombres:</label>
          <input type="text" class="form-control" name="fname" placeholder="Juan Andres" required>
      </div>

      <div class="form-group">
        <label >Apellidos:</label>
          <input type="text" class="form-control" name="lname" placeholder="Perez Rodríguez" required>
        
      </div>

      <div class="form-group">
        <label >Cédula de Identidad:</label>
          <input type="text" class="form-control" name="lname" placeholder="12.345.678" required>
        
      </div>

      <div class="form-group">
        <label for="lists" >Sexo:</label>
          <select id="lists" class="form-control">
            <option value="Maculino">Masculino</option>
            <option value="Femenino">Femenino</option>
          </select>
        
      </div>

      <div class="form-group">
        <label >Fecha de Nacimiento:</label>
          <input type="date" class="form-control" name="bdate" required>
        
      </div>

      <div class="form-group">
        <label >Correo:</label>
          <input type="email" class="form-control" name="mail" placeholder="name@example.com" required>
        
      </div>

      <div class="form-group">
        <label>Contraseña:</label>
          <input type="password" class="form-control" name="pass" required>
        
      </div>

      <div class="form-group">
        <label >Confirmar Contraseña:</label>
          <input type="password" class="form-control" name="pass_c" required>
        
      </div>
    </form>
<!-- Ends Body Content-->
    </div>

        <div class="modal-footer">
          <center>
          <button type="submit" class="btn btn-success btn-block" data-dismiss="modal">Registrarse</button>
          </center>
        </div>
      </div>
      
    </div>
  </div><!-- Ends Modal Reg Asp-->




  <!--Registrarse como Empresa-->

  <div class="modal fade" id="myModalregemp" role="dialog" >
    <div class="modal-dialog ">
    
      <!-- Modal content-->
      <div class="modal-content">

        <div class="modal-header" style="color: white; background-color: #006bb3">
          <button type="button" class="close" data-dismiss="modal">&times;</button>

          <h4 class="modal-title text-center" >Registro - Empresa</h4>
        </div>

    <div id="mod_reg" class="modal-body" >
<!-- Body Content-->
     <form class="form-horizontal">

      <div class="form-group">
          <label>Nombre de la empresa:</label>
          <input type="text" class="form-control" name="fname" placeholder="Nombre Empresa, C.A." required>
      </div>

      <div class="form-group">
        <label >RIF:</label>
          <input type="text" class="form-control" name="lname" placeholder="J-12345678" required>
        
      </div>

      <div class="form-group">
        <label >Correo:</label>
          <input type="email" class="form-control" name="mail" placeholder="name@example.com" required>
        
      </div>

      <div class="form-group">
        <label>Contraseña:</label>
          <input type="password" class="form-control" name="pass" required>
        
      </div>

      <div class="form-group">
        <label >Confirmar Contraseña:</label>
          <input type="password" class="form-control" name="pass_c" required>
        
      </div>
    </form>
<!-- Ends Body Content-->
    </div>

        <div class="modal-footer">
          <center>
          <button type="submit" class="btn btn-success btn-block" data-dismiss="modal">Registrarse</button>
          </center>
        </div>
      </div>
      
    </div>
  </div><!-- Ends Modal Reg Emp-->






