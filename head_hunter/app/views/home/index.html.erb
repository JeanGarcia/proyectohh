  <!--Modals-->

    <!-- Iniciar Sesión -->
  <div class="modal fade" id="myModalinit" role="dialog" >
    <div class="modal-dialog">
    
      <!-- Modal content-->
      <div class="modal-content">

        <div class="modal-header" style="color: white; background-color: #006bb3">
